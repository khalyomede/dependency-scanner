{
  "name": "khalyomede/dependency-scanner",
  "description": "Returns a list of packages available for updates.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Khalyomede",
      "email": "khalyomede@gmail.com"
    }
  ],
  "keywords": [
    "dependency",
    "check",
    "version",
    "out of date",
    "composer",
    "packages",
    "update"
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0.0",
    "composer/semver": "2.*",
    "guzzlehttp/guzzle": "6.*"
  },
  "autoload": {
    "psr-4": {
      "Khalyomede\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "9.*"
  },
  "scripts": {
    "test": "phpunit"
  }
}
